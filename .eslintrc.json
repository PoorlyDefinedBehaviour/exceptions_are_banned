{
  "env": {
    "browser": false,
    "es6": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": [2, { "args": "all", "argsIgnorePattern": "^_" }]
  }
}
